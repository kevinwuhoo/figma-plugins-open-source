ect_thumbnail_url":"/community/plugin/982732544385962656/thumbnail","install_status":null,"profile_install_status":null,"publisher":{"id":"697643","team_id":null,"org_id":null,"location":"Washington DC","profile_handle":"aashreys","public_at":"2020-12-02T21:05:54.351Z","follower_count":1,"following_count":3,"primary_user_id":"620716149748585546","name":"Aashrey Sharma","img_url":"https://s3-alpha.figma.com/profile/c7023d3c-abd5-4ff7-a0a9-ed58bb99d78c","img_urls":{"120_120":"https://s3-alpha.figma.com/profile/c7023d3c-abd5-4ff7-a0a9-ed58bb99d78c","500_500":"https://s3-alpha.figma.com/profile/2294fd8c-113c-4d44-93a5-dbedac162169"},"current_user_is_following":null,"current_user_is_followed":null,"is_restricted_by_current_user":null,"realtime_token":"/profile-697643:1623385932:0:2b6d9992f3a3dc0d3008cf97fa7cf49c18a9c019"},"roles":{"is_public":true},"versions":{"18187":{"id":"18187","plugin_id":"982732544385962656","name":"Check Character Limit","description":"<p>Check Character Limit is a plugin to set character limits for text layers&nbsp;and highlight any layers that exceed their limits. It is especially handy for enforcing character limits for localization.</p><p><br></p><h1>Usage</h1><ol><li>Add <code>[CharLimit:XX]</code> to the name of your text layers. XX is the maximum character count for the layer. For example: <code>Description Text [CharLimit:2000]</code>.</li><li>Edit content and run the plugin to check your text layers (see note below).</li><li>Text layers that exceed their limits will be flagged in red. Check the top of your layer list to locate the flags.</li><li>Select a flag from the layer list and press Shift + 2 to locate it visually.</li><li>Edit the text layer to fit your character limit.</li></ol><p><br></p><h2>Note</h2><p>When running the plugin, if no layer is selected, the whole page will be checked. To check a specific layer, or all layers under a frame, select the layers or frames before running the plugin.</p><p><br></p><p>This allows you to scope the checking as you see fit. It's also useful for performance since checking large pages takes longer.</p><p><br></p><h1>Open Source</h1><p>This plugin is open source and can be found <a href=\"https://github.com/aashreys/figma-check-character-count\" rel=\"noreferrer noopener nofollow\" target=\"_blank\">here</a>.</p>","version":"4","icon_path":"plugins/982732544385962656/18187/44437205/22a167d9-38c2-4716-a4fa-b24c48478175-icon","cover_image_path":"plugins/982732544385962656/18179/50645930/5625c9ed-8166-4006-869b-7e0933be352e-cover","code_path":"plugins/982732544385962656/18187/22a167d9-38c2-4716-a4fa-b24c48478175-code","resource_staging_signature":"22a167d9-38c2-4716-a4fa-b24c48478175","manifest":{"id":"982732544385962656","api":"1.0.0","main":"code.js","name":"Check Character Limit"},"release_notes":"Increased the amount of time toast messages are displayed","created_at":"2021-06-06T17:56:51.901Z","redirect_icon_url":"/community/plugin/982732544385962656/icon","redirect_cover_image_url":"/community/plugin/982732544385962656/thumbnail","redirect_code_url":"/community/plugin/982732544385962656/code"}},"creator":{"id":"620716149748585546","handle":"Aashrey Sharma","img_url":"https://s3-alpha.figma.com/profile/c7023d3c-abd5-4ff7-a0a9-ed58bb99d78c"},"current_user_liked":null,"community_publishers":{"accepted":[{"id":"697643","team_id":null,"org_id":null,"location":"Washington DC","profile_handle":"aashreys","public_at":"2020-12-02T21:05:54.351Z","follower_count":1,"following_count":3,"primary_user_id":"620716149748585546","name":"Aashrey Sharma","img_url":"https://s3-alpha.figma.com/profile/c7023d3c-abd5-4ff7-a0a9-ed58bb99d78c","img_urls":{"120_120":"https://s3-alpha.figma.com/profile/c7023d3c-abd5-4ff7-a0a9-ed58bb99d78c","500_500":"https://s3-alpha.figma.com/profile/2294fd8c-113c-4d44-93a5-dbedac162169"},"current_user_is_following":null,"current_user_is_followed":null,"is_restricted_by_current_user":null,"realtime_token":"/profile-697643:1623385932:0:2b6d9992f3a