er.\u003c/strong\u003e\u003c/li\u003e\u003c/ul\u003e\u003cp\u003e\u003cbr\u003e\u003c/p\u003e","version":"1","icon_path":"plugins/1075424703504231427/28248/96c000d9-6a43-485c-b94c-e8947968bac6-icon","cover_image_path":"plugins/1075424703504231427/28248/96c000d9-6a43-485c-b94c-e8947968bac6-cover","code_path":"plugins/1075424703504231427/28248/96c000d9-6a43-485c-b94c-e8947968bac6-code","resource_staging_signature":"96c000d9-6a43-485c-b94c-e8947968bac6","manifest":{"id":"1075424703504231427","ui":"ui.html","api":"1.0.0","main":"code.js","menu":[{"name":"Float selection","command":"float"},{"separator":true},{"name":"Previous","command":"previous"}],"name":"Floating","editorType":["figma","figjam"]},"release_notes":"","created_at":"2022-02-16T14:15:53.764Z","snapshot_path":null,"redirect_icon_url":"/community/plugin/1075424703504231427/icon","redirect_cover_image_url":"/community/plugin/1075424703504231427/thumbnail","redirect_code_url":"/community/plugin/1075424703504231427/code","redirect_snapshot_url":null,"current_plugin_version_id":"28248"}},"creator":{"id":"372256183912319549","handle":"Tank","img_url":"https://s3-alpha.figma.com/profile/378e95e0-6278-4f8e-8e92-7c9f8b4544db"},"current_user_liked":null,"community_publishers":{"accepted":[{"id":"61240","team_id":null,"org_id":null,"location":"Shenzhen, China","profile_handle":"tankxu","public_at":"2021-05-24T07:22:38.991Z","follower_count":3,"following_count":9,"primary_user_id":"372256183912319549","name":"Tank","img_url":"https://s3-alpha.figma.com/profile/378e95e0-6278-4f8e-8e92-7c9f8b4544db","img_urls":{"120_120":"https://s3-alpha.figma.com/profile/378e95e0-6278-4f8e-8e92-7c9f8b4544db","500_500":"https://s3-alpha.figma.com/profile/4760eb7d-6924-4ffd-a16d-2f41a54996ef"},"current_user_is_following":null,"current_user_is_followed":null,"is_restricted_by_current_user":null,"realtime_token":"/profile-61240:1645677010:0:f8ec014ac7b7b4bea1fe5d43a49ea0787c9affe4"}]}},{"id":"1078011857203492445","created_at":"2022-02-22T